{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "consistent-return": "off",
        "no-console": 0,
        "import/extensions": 0,
        "class-methods-use-this":"off",
        "no-unused-vars":"off",
        "func-names":"off",
        "no-underscore-dangle":"off"
    }
}
